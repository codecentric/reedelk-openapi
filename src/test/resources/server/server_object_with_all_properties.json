{
  "url": "https://{environment}.{domain}.com/v1",
  "description": "Development server",
  "variables": {
    "environment": {
      "description": "Environment variable",
      "default": "dev",
      "enum": [
        "dev",
        "uat",
        "prod"
      ]
    },
    "domain": {
      "description": "Domain variable",
      "default": "localhost",
      "enum": [
        "localhost",
        "mydomain1",
        "mydomain2"
      ]
    }
  }
}
